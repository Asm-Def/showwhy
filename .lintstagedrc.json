{
	"*.{json,md}": ["essex prettify --staged"],
	"javascript/**/*.*.{js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
		"essex prettify --staged",
		"essex lint --fix --staged"
	],
	"python/showwhy-backend/**/*": [
		"cd python/showwhy-backend && poetry run pre-commit"
	],
	"python/showwhy-interface/**/*": [
		"cd python/showwhy-interface && poetry run pre-commit"
	]
}
